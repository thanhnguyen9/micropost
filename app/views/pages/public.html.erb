
<div class="top-page"></div>

  <div class="top-page"></div>
    <div class="col-md-2 well" >
      <%= render partial: "profile" %>
  </div>


<div class="col-md-7 well">

  <h1>Hey <%= @current_user.username.capitalize %>. Recently Posts</h1>

  <% @posts.each do |post| %>

        <%= image_tag post.user.photo_url, class: "public-image" %>
        <%= link_to post.user.username, posts_show_path(id: post.user_id) %>
          <ul>
            <li>
              <%= post.tweet %>
              <%= post.create_at.strftime("%b %e, %l:%M %p") %>
            </li>
          </ul>
  <%end%>

  <%= paginate @posts %>
</div>


<div class="col-md-2 well game">
  <%= render partial: "game" %>
  </div>
