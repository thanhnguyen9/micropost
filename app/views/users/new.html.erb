<h1>Please Sign Up</h1>

<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %><br /><br />

  <%= f.label :password %>
  <%= f.password_field :password %><br /><br />



  <%= f.label :photo_url %>
  <%= f.text_field :photo_url %><br /><br />

  <%= f.submit "Create Account" %>
<%end%>
